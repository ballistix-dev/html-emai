
'.text.html.basic':
  'EDM: Button Block':
    'prefix': 'edm:button-block'
    'body': """
              <!-- Button : BEGIN -->
              <table role="presentation" cellspacing="0" cellpadding="0" border="0" align="center" style="margin: auto" width="100%">
                  <tr>
                      <td style="background: #222222; text-align: center;" class="button-td">
                          <a href="http://www.google.com" style="background: #222222; border: 15px solid #222222; font-family: sans-serif; font-size: 13px; line-height: 110%; text-align: center; text-decoration: none; display: block; font-weight: bold;" class="button-a">
                              &nbsp;&nbsp;&nbsp;&nbsp;<span style="color:#ffffff;">A Button</span>&nbsp;&nbsp;&nbsp;&nbsp;
                          </a>
                      </td>
                  </tr>
              </table>
              <!-- Button : END -->
            """
  'EDM: Button Center':
    'prefix': 'edm:button-center'
    'body': """
              <!-- Button : BEGIN -->
              <table role="presentation" cellspacing="0" cellpadding="0" border="0" align="center" style="margin: auto">
                  <tr>
                      <td style="background: #222222; text-align: center;" class="button-td">
                          <a href="http://www.google.com" style="background: #222222; border: 15px solid #222222; font-family: sans-serif; font-size: 13px; line-height: 110%; text-align: center; text-decoration: none; display: block; font-weight: bold;" class="button-a">
                              &nbsp;&nbsp;&nbsp;&nbsp;<span style="color:#ffffff;">A Button</span>&nbsp;&nbsp;&nbsp;&nbsp;
                          </a>
                      </td>
                  </tr>
              </table>
              <!-- Button : END -->
            """
  'EDM: Button Default':
    'prefix': 'edm:button'
    'body': """
              <!-- Button : BEGIN -->
                <table role="presentation" cellspacing="0" cellpadding="0" border="0" class="center-on-narrow" style="float:left;">
                    <tr>
                        <td style="background: #222222; text-align: center;" class="button-td">
                            <a href="http://www.google.com" style="background: #222222; border: 15px solid #222222; font-family: sans-serif; font-size: 13px; line-height: 110%; text-align: center; text-decoration: none; display: block; font-weight: bold;" class="button-a">
                              <span style="color:#ffffff;" class="button-link">&nbsp;&nbsp;&nbsp;&nbsp;A Button&nbsp;&nbsp;&nbsp;&nbsp;</span>
                          </a>
                      </td>
                  </tr>
              </table>
              <!-- Button : END -->
            """
  'EDM: Column 1':
    'prefix': 'edm:col-1'
    'body': """
              <!-- 1 Column Text : BEGIN -->
              <tr>
                  <td bgcolor="#ffffff">
                      <table role="presentation" cellspacing="0" cellpadding="0" border="0" width="100%">
                          <tr>
                              <td style="padding: 40px; font-family: sans-serif; font-size: 15px; line-height: 140%; color: #555555;">
                                  $1
                              </td>
                          </tr>
                      </table>
                  </td>
              </tr>
              <!-- 1 Column Text : END -->
            """
  'EDM: Column 2':
    'prefix': 'edm:col-2'
